\n                    history: this.tourService.getHistory()\r\n                });\r\n            } else {\r\n                this.break.emit({\r\n                    tourEvent: 'break',\r\n                    index: this.tourService.getLastStep().index!,\r\n                    history: this.tourService.getHistory()\r\n                });\r\n            }\r\n            this.tourService.stopTour();\r\n        };\r\n    }\r\n}\r\n"]},"resolvedIds":{"@angular/core":{"external":true,"id":"@angular/core","meta":{},"moduleSideEffects":true,"syntheticNamedExports":false},"@angular/common":{"external":true,"id":"@angular/common","meta":{},"moduleSideEffects":true,"syntheticNamedExports":false},"../../public_api":{"external":false,"id":"C:\\Documents\\GitHub\\ng3-tour\\dist\\ng3-tour\\esm2020\\public_api.mjs","meta":{},"moduleSideEffects":true,"syntheticNamedExports":false}},"sourcemapChain":[],"syntheticNamedExports":false,"transformDependencies":[]},{"ast":{"type":"Program","start":0,"end":5410,"body":[{"type":"ImportDeclaration","start":0,"end":41,"specifiers":[{"type":"ImportSpecifier","start":9,"end":17,"imported":{"type":"Identifier","start":9,"end":17,"name":"NgModule"},"local":{"type":"Identifier","start":9,"end":17,"name":"NgModule"}}],"source":{"type":"Literal","start":25,"end":40,"value":"@angular/core","raw":"'@angular/core'"}},{"type":"ImportDeclaration","start":43,"end":90,"specifiers":[{"type":"ImportSpecifier","start":52,"end":64,"imported":{"type":"Identifier","start":52,"end":64,"name":"CommonModule"},"local":{"type":"Identifier","start":52,"end":64,"name":"CommonModule"}}],"source":{"type":"Literal","start":72,"end":89,"value":"@angular/common","raw":"'@angular/common'"}},{"type":"ImportDeclaration","start":92,"end":139,"specifiers":[{"type":"ImportSpecifier","start":101,"end":113,"imported":{"type":"Identifier","start":101,"end":113,"name":"RouterModule"},"local":{"type":"Identifier","start":101,"end":113,"name":"RouterModule"}}],"source":{"type":"Liter